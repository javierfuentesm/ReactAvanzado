{
  "version": 2,
  "alias": ["petgram-rjfm"],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    
    {
    "src": "(.*).js", "dest": "$1.js" },
    {
    "src": "(.*).json", "dest": "$1.json" },
    {
    "src": "(.*).png", "dest": "$1.png" },
    {
    "src": "/.*", "dest": "index.html" }
  ]
}
